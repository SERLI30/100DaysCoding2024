
package Days91;

import java.util.Scanner;

public class Days91 {
    public static void main(String[] args) {
        Scanner serli = new Scanner(System.in);
        while (true) {
            System.out.print("Masukkan angka: ");
            int a = serli.nextInt();
            if (a % 2 == 0) {
                System.out.println("Genap");
            } else {
                System.out.println("You and I, end");
                break;
            }
        }
       
    }
}
