
package Days66;

import java.util.Scanner;

public class Days66 {
    public static void main(String[] args) {
        Scanner serli = new Scanner(System.in);
        System.out.print("Masukkan ukuran sisi persegi: ");
        int n = serli.nextInt();

        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                System.out.print("* ");
            }
            System.out.println();
        }

    }
}
